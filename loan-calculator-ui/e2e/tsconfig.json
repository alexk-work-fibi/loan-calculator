{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "dist/e2e",
    "strict": true,
    "lib": ["es2022", "DOM"],
    "types": ["jasmine", "node", "selenium-webdriver"]
  },
  "include": ["e2e/**/*.ts"],
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  }
}
